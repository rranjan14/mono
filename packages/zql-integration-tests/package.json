{
  "name": "zql-integration-tests",
  "description": "Integration tests of ZQL, ZQLite, ZPG",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-types": "tsc",
    "check-types:watch": "tsc --watch",
    "build:bin": "tsc -p tsconfig.bin.json",
    "lint": "oxlint --type-aware src/",
    "test": "vitest run",
    "test:pg17": "vitest run --project='*17*'",
    "test:watch": "vitest watch",
    "test:pg17:watch": "vitest watch --project='*17*'",
    "bench": "vitest bench",
    "test-types": "vitest run --typecheck.only --no-browser.enabled",
    "test-types:watch": "vitest watch --typecheck.only --no-browser.enabled"
  },
  "devDependencies": {
    "@rocicorp/prettier-config": "^0.4.0",
    "@types/tmp": "^0.2.6",
    "otel": "0.0.0",
    "shared": "0.0.0",
    "typescript": "~5.9.3",
    "z2s": "0.0.0",
    "zero-cache": "0.0.0",
    "zero-protocol": "0.0.0",
    "zero-schema": "0.0.0",
    "zero-server": "0.0.0",
    "zql": "0.0.0",
    "zqlite": "0.0.0",
    "zero-types": "0.0.0"
  },
  "prettier": "@rocicorp/prettier-config"
}
