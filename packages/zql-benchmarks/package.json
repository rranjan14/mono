{
  "name": "zql-benchmarks",
  "description": "Benchmarks of ZQL, ZQLite, ZPG",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-types": "tsc",
    "check-types:watch": "tsc --watch",
    "build:bin": "tsc -p tsconfig.bin.json",
    "lint": "oxlint --type-aware src/",
    "test": "vitest run --silent=false --disable-console-intercept",
    "test:watch": "vitest watch",
    "bench": "vitest bench --disable-console-intercept --run",
    "bench:bmf": "BENCH_OUTPUT_FORMAT=json npm run bench 2>&1 | npx tsx ../shared/src/tool/mitata-json-to-bmf.ts",
    "bench:bmf:silent": "npm run bench:bmf --silent",
    "test-types": "vitest run --typecheck.only --no-browser.enabled",
    "test-types:watch": "vitest watch --typecheck.only --no-browser.enabled"
  },
  "devDependencies": {
    "@rocicorp/prettier-config": "^0.4.0",
    "@types/tmp": "^0.2.6",
    "otel": "0.0.0",
    "shared": "0.0.0",
    "typescript": "~5.9.3",
    "zero-cache": "0.0.0",
    "zero-protocol": "0.0.0",
    "zero-types": "0.0.0",
    "zql": "0.0.0",
    "zero-schema": "0.0.0",
    "zqlite": "0.0.0",
    "zql-integration-tests": "0.0.0"
  },
  "prettier": "@rocicorp/prettier-config",
  "dependencies": {
    "mitata": "^1.0.34"
  }
}
